pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Build') {
            steps {
                echo 'Building application'
            }
        }
         stage('Deploy') {
            steps {
                echo 'Deploying application'
            }
        }
         stage('Test') {
            steps {
                echo 'Testing application'
            }
        }
         stage('Release') {
            steps {
                echo 'Final Release'
            }
        }
    }
}
